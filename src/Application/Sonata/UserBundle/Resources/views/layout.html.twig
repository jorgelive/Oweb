{% extends "::global.html.twig" %}
{% block stylesheets %}
    {% stylesheets '@ApplicationSonataUserBundle/Resources/public/css/*' filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock stylesheets%}
{% block javascripts %}
    {% javascripts '@ApplicationSonataUserBundle/Resources/public/js/*' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock javascripts %}
{% block container %}
    <div id="ApplicationSonataUserBundle">
        {% block fos_user_content %}
        {% endblock fos_user_content %}
    </div>
{% endblock container %}
{% block javascriptsfinal %}{% endblock javascriptsfinal %}
