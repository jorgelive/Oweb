{% extends 'GoproInventarioBundle::layout.html.twig' %}
{% block title %}Actualizar item{% endblock title %}
{% block titulo %}Actualizar item{% endblock titulo %}
{% block content %}
    {{ form(edit_form) }}
    <ul class="registroAcciones">
        {% if edit_form.vars.value.id %}
            <li>
                <a class="boton" href="{{ path('gopro_inventario_item_show', { 'id': edit_form.vars.value.id }) }}">
                    Ir al item
                </a>
            </li>
        {% endif %}
        <li>
            <a class="boton" href="{{ path('gopro_inventario_item') }}">
                Volver a la lista
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
{% endblock content %}
