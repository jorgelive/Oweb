{
    "name": "gopro/excelbundle",
    "description": "This is a Symfony2 Bundle helps you to read and write Excel files (including pdf, xlsx, odt), thanks to the PHPExcel library",
    "authors": [
        {
            "name": "Jorge Gomez",
            "email": "jorge@live.com.pe"
        },
        {
            "name": "ExcelBundle Contributors",
            "homepage": "https://github.com/jorgelive/ExcelBundle#contributors"
        }
    ],

    "keywords": ["xls","Excel", "symfony2", "bundle"],
    "homepage": "http://www.welcometothebundle.com",
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "phpoffice/phpexcel": "1.7.9"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "symfony/finder": "~2.0",
        "symfony/form": "~2.1",
        "symfony/class-loader": "~2.1",
        "symfony/validator": "~2.1",
        "symfony/browser-kit": "~2.1",
        "sensio/framework-extra-bundle": "~2.1"
    },

    "autoload": {
        "psr-0": {
            "Gopro\\ExcelBundle": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "target-dir": "Gopro/ExcelBundle",
    "config": {
        "bin-dir": "bin"
    }
}
