<div class="btn-group btn-group-justified btn-group-md">
    {% for item in items %}
        {% if item.route is defined %}
            <a class="btn btn-primary" href="{{ path(item.route) }}">
                {{ item.nombre }}
            </a>
        {% elseif item.url %}
            <a class="btn btn-primary" href="http://{{ item.url }}">
                {{ item.nombre }}
            </a>
        {% else %}

        {% endif %}
    {% endfor %}
</div>

