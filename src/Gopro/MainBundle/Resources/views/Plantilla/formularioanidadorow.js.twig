<div>
    <div id="{%=o.formularioNombre%}_{%=o.entidadAnidada%}_{%=o.id%}">
        <div>
            <label {% if (o.opcional==false) { %}class="required" {% } %}for="{%=o.formularioNombre%}_{%=o.entidadAnidada%}_{%=o.id%}_nombremostrar">{%=o.nombreTipo%}</label>
            {% if (o.tipo=='texto') { %}
                <input id="{%=o.formularioNombre%}_{%=o.entidadAnidada%}_{%=o.id%}_{%=o.tipo%}" type="text" value="{%=o.valor%}" maxlength="255" {% if (o.opcional==false) { %}required="required" {% } %}name="{%=o.formularioNombre%}[{%=o.entidadAnidada%}][{%=o.id%}][{%=o.tipo%}]">
            {% } else if(o.tipo=='numero') { %}
                <input id="{%=o.formularioNombre%}_{%=o.entidadAnidada%}_{%=o.id%}_{%=o.tipo%}" type="number" value="{%=o.valor%}" maxlength="12" {% if (o.opcional==false) { %}required="required" {% } %}name="{%=o.formularioNombre%}[{%=o.entidadAnidada%}][{%=o.id%}][{%=o.tipo%}]">
            {% } else if(o.tipo=='fecha') { %}
                <input id="{%=o.formularioNombre%}_{%=o.entidadAnidada%}_{%=o.id%}_{%=o.tipo%}" class="datePicker" type="date" value="{%=o.valor%}" maxlength="12" {% if (o.opcional==false) { %}required="required" {% } %}name="{%=o.formularioNombre%}[{%=o.entidadAnidada%}][{%=o.id%}][{%=o.tipo%}]">
            {% } else if(o.tipo=='archivocargado') { %}
                <input id="{%=o.formularioNombre%}_{%=o.entidadAnidada%}_{%=o.id%}_{%=o.tipo%}" type="file" {% if (o.opcional==false) { %}required="required" {% } %}name="{%=o.formularioNombre%}[{%=o.entidadAnidada%}][{%=o.id%}][{%=o.tipo%}]">
            {% } %}
            <input type="hidden" value="{%=o.valorTipo%}" name="{%=o.formularioNombre%}[{%=o.entidadAnidada%}][{%=o.id%}][{%=o.campoCaracteristica%}]">
        </div>
    </div>
</div>