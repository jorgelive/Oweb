{% extends 'GoproVipacReporteBundle::layout.html.twig' %}
{% block content %}
    <h1>Sentencia SQL</h1>
    <table class="registroPropiedades">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Descripci√≥n</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Contenido</th>
                <td>{{ entity.contenido }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="registroAcciones">
        <li>
            <a href="{{ path('sentencia') }}">
                Volver a la lista
            </a>
        </li>
        <li>
            <a href="{{ path('sentencia_edit', { 'id': entity.id }) }}">
                Editar
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
{% endblock content %}
