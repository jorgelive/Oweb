{% extends 'GoproVipacReporteBundle::layout.html.twig' %}
{% block title %}Parametro{% endblock title %}
{% block titulo %}Parametro{% endblock titulo %}
{% block content %}
    <table class="registroPropiedades">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Sentencia</th>
                <td>{{ entity.sentencia.nombre }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Contenido</th>
                <td>{{ entity.contenido }}</td>
            </tr>
            <tr>
                <th>Usuario</th>
                <td>{{ entity.user.username }}</td>
            </tr>
            <tr>
                <th>PÃºblico</th>
                <td>{{ entity.publico }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="registroAcciones">
        <li>
            <a class="boton" href="{{ path('gopro_vipac_reporte_parametro') }}">
                Volver a la lista
            </a>
        </li>
        <li>
            <a class="boton" href="{{ path('gopro_vipac_reporte_parametro_edit', { 'id': entity.id }) }}">
                Editar
            </a>
        </li>
        <li>
            <a class="boton" href="{{ path('gopro_vipac_reporte_parametro_new') }}">
                Crear otro
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
{% endblock content %}
